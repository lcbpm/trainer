    "version": "0.2.0",
    "configurations": [

        {
            "name":"Python: audio video",
            "type": "debugpy",
            "request":"launch",
            "program":"${workspaceFolder}/audio/video.py",
            "args": [
                // "--proxy",
                // "http://localhost:7890",
                // "--local_model_path",
                // "${workspaceFolder}/audio/models/models--stabilityai-stable-audio-open-1.0"
                // 使用环境变量存储token，而不是硬编码
                // "--token",
                // "${env:HUGGING_FACE_TOKEN}"
            ],
            "console":"integratedTerminal",
            "python": "${workspaceFolder}/.venv/bin/python3.11"
        },
        {
            "name":"Python: audio cli",
            "type": "debugpy",
            "request":"launch",
            "program":"${workspaceFolder}/audio/cli.py",
            "args": [
                // "--proxy",
                // "http://localhost:7890",
                "--local_model_path",
                "${workspaceFolder}/audio/models/models--stabilityai-stable-audio-open-1.0"
                // 使用环境变量存储token，而不是硬编码
                // "--token",
                // "${env:HUGGING_FACE_TOKEN}"
            ],
            "console":"integratedTerminal",
            "python": "${workspaceFolder}/.venv/bin/python3.11"
        },
        {
            "name": "Python: finetuning",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/peft/finetuning.py",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.trainer/bin/python3.11"
        },
        {
            "name": "Python: inference",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/peft/inference.py",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.trainer/bin/python3.11"
        },
        {
            "name":"Python: generator",
            "type": "debugpy",
            "request":"launch",
            "program":"${workspaceFolder}/image/generator.py",
            "console":"integratedTerminal",
            "python": "${workspaceFolder}/.trainer/bin/python3.11"
        }
    ]
}